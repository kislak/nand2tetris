# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore the database.yml
config/database.yml

# Ignore the node_modules/ folder
/node_modules
/app/assets/node_modules

# Ignore dotenv files
/.env
